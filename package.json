{
  "name": "dexai",
  "version": "1.0.0",
  "description": "A smart agent which the capability to interact with the blockchain",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Apexloaded/dexai.git"
  },
  "author": "Apexloaded",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "ci": "pnpm run build && pnpm run check-format && pnpm run check-exports && pnpm run lint && pnpm run test",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack .",
    "lint": "tsc",
    "test": "jest",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "bugs": {
    "url": "https://github.com/Apexloaded/dexai/issues"
  },
  "homepage": "https://github.com/Apexloaded/dexai#readme",
  "keywords": [
    "dexai",
    "dex",
    "exchange",
    "ai",
    "dexgpt",
    "sonic",
    "blockchain",
    "agent",
    "sonic-agent",
    "smart-agent",
    "smart",
    "langchain"
  ],
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.3",
    "@changesets/cli": "^2.28.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "prettier": "^3.5.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@heyanon/sdk": "^2.0.4",
    "@langchain/core": "^0.3.40",
    "reflect-metadata": "^0.2.2",
    "viem": "^2.23.3",
    "zod": "^3.24.2"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  }
}